<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloudtimes.account.mapper.CtAgentDividendSettlementMapper">
    <sql id="selectCtAgentDividendSettlementVo">
        select * from ct_agent_dividend_settlement
    </sql>

    <select id="selectCtAgentDividendSettlementList" parameterType="CtAgentDividendSettlement" resultMap="CtAgentDividendSettlementResult">
        <include refid="selectCtAgentDividendSettlementVo"/>
        <where>  
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="storeId != null  and storeId != ''"> and store_id = #{storeId}</if>
            <if test="dividendYear != null "> and dividend_year = #{dividendYear}</if>
            <if test="dividendMonth != null "> and dividend_month = #{dividendMonth}</if>
            and del_flag = '0'
        </where>
    </select>
    
    <select id="selectCtAgentDividendSettlementById" parameterType="String" resultMap="CtAgentDividendSettlementResult">
        <include refid="selectCtAgentDividendSettlementVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertCtAgentDividendSettlement" parameterType="CtAgentDividendSettlement">
        insert into ct_agent_dividend_settlement
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="userId != null">user_id,</if>
            <if test="storeId != null">store_id,</if>
            <if test="dividendYear != null">dividend_year,</if>
            <if test="dividendMonth != null">dividend_month,</if>
            <if test="settleAmount != null">settle_amount,</if>
            <if test="isAgentOk != null">is_agent_ok,</if>
            <if test="isPlatformOk != null">is_platform_ok,</if>
            <if test="state != null">state,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="platformApprovedTime != null">platform_approved_time,</if>
            <if test="agentApprovedTime != null">agent_approved_time,</if>
            <if test="createDate != null">create_date,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="userId != null">#{userId},</if>
            <if test="storeId != null">#{storeId},</if>
            <if test="dividendYear != null">#{dividendYear},</if>
            <if test="dividendMonth != null">#{dividendMonth},</if>
            <if test="settleAmount != null">#{settleAmount},</if>
            <if test="isAgentOk != null">#{isAgentOk},</if>
            <if test="isPlatformOk != null">#{isPlatformOk},</if>
            <if test="state != null">#{state},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="platformApprovedTime != null">#{platformApprovedTime},</if>
            <if test="agentApprovedTime != null">#{agentApprovedTime},</if>
            <if test="createDate != null">#{createDate},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateCtAgentDividendSettlement" parameterType="CtAgentDividendSettlement">
        update ct_agent_dividend_settlement
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="storeId != null">store_id = #{storeId},</if>
            <if test="dividendYear != null">dividend_year = #{dividendYear},</if>
            <if test="dividendMonth != null">dividend_month = #{dividendMonth},</if>
            <if test="settleAmount != null">settle_amount = #{settleAmount},</if>
            <if test="isAgentOk != null">is_agent_ok = #{isAgentOk},</if>
            <if test="isPlatformOk != null">is_platform_ok = #{isPlatformOk},</if>
            <if test="state != null">state = #{state},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="platformApprovedTime != null">platform_approved_time = #{platformApprovedTime},</if>
            <if test="agentApprovedTime != null">agent_approved_time = #{agentApprovedTime},</if>
            <if test="createDate != null">create_date = #{createDate},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>
</mapper>
