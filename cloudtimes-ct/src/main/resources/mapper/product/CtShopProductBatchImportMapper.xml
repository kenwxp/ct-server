<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloudtimes.product.mapper.CtShopProductBatchImportMapper">
    

    <sql id="selectCtShopProductBatchImportVo">
        select * from ct_shop_product_batch_import
    </sql>

    <select id="selectCtShopProductBatchImportList" parameterType="CtShopProductBatchImport" resultType="CtShopProductBatchImport">
        <include refid="selectCtShopProductBatchImportVo"/>
        <where>  
            <if test="fileName != null  and fileName != ''"> and file_name like concat('%', #{fileName}, '%')</if>
            <if test="shopNo != null  and shopNo != ''"> and shop_no = #{shopNo}</if>
        </where>
    </select>
    
    <select id="selectCtShopProductBatchImportByFileName" parameterType="String" resultType="CtShopProductBatchImport">
        <include refid="selectCtShopProductBatchImportVo"/>
        where file_name = #{fileName}
    </select>
        
    <insert id="insertCtShopProductBatchImport" parameterType="CtShopProductBatchImport">
        insert into ct_shop_product_batch_import
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="fileName != null">file_name,</if>
            <if test="shopNo != null and shopNo != ''">shop_no,</if>
            <if test="batchNo != null and batchNo != ''">batch_no,</if>
            <if test="filePath != null and filePath != ''">file_path,</if>
            <if test="totalNum != null">total_num,</if>
            <if test="successNum != null">success_num,</if>
            <if test="failNum != null">fail_num,</if>
            <if test="isImported != null and isImported != ''">is_imported,</if>
            <if test="isSuccess != null and isSuccess != ''">is_success,</if>
            <if test="remark != null">remark,</if>
            <if test="uploadDate != null">upload_date,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="fileName != null">#{fileName},</if>
            <if test="shopNo != null and shopNo != ''">#{shopNo},</if>
            <if test="batchNo != null and batchNo != ''">#{batchNo},</if>
            <if test="filePath != null and filePath != ''">#{filePath},</if>
            <if test="totalNum != null">#{totalNum},</if>
            <if test="successNum != null">#{successNum},</if>
            <if test="failNum != null">#{failNum},</if>
            <if test="isImported != null and isImported != ''">#{isImported},</if>
            <if test="isSuccess != null and isSuccess != ''">#{isSuccess},</if>
            <if test="remark != null">#{remark},</if>
            <if test="uploadDate != null">#{uploadDate},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateCtShopProductBatchImport" parameterType="CtShopProductBatchImport">
        update ct_shop_product_batch_import
        <trim prefix="SET" suffixOverrides=",">
            <if test="shopNo != null and shopNo != ''">shop_no = #{shopNo},</if>
            <if test="batchNo != null and batchNo != ''">batch_no = #{batchNo},</if>
            <if test="filePath != null and filePath != ''">file_path = #{filePath},</if>
            <if test="totalNum != null">total_num = #{totalNum},</if>
            <if test="successNum != null">success_num = #{successNum},</if>
            <if test="failNum != null">fail_num = #{failNum},</if>
            <if test="isImported != null and isImported != ''">is_imported = #{isImported},</if>
            <if test="isSuccess != null and isSuccess != ''">is_success = #{isSuccess},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="uploadDate != null">upload_date = #{uploadDate},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where file_name = #{fileName}
    </update>
</mapper>